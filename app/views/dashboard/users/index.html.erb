<div class="w-75 mt-2">   
  <h1>テナント一覧</h1>
  <table class="table table-responsive mt-5">

    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">テナント名</th>
        <th scope="col">電話番号</th>
        <th scope="col">Email</th>
        
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <th scope="row"><%= user.id %></td>
        <td><%= user.company_name %></td>
        <td><%= user.phone_number %></td> 
        <td><%= user.email %></td>
        
        
      </tr>
      <% end %>
    </tbody>
  </table>
</div> 